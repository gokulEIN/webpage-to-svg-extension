{
  "manifest_version": 3,
  "name": "Webpage to SVG",
  "version": "1.0",
  "description": "Capture entire web pages and download as SVG.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}
